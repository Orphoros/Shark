let a = [1, 2, 10, 4];

let double = (x = 20) => { x * 2 };

puts(double(2));

let map = (arr, f) => {
    let iter = (arr, accumulated) => {
        if (len(arr) == 0) {
            accumulated
        } else {
            iter(rest(arr), push(accumulated, f(first(arr))))
        }
    } 
    iter(arr, []);
};

let result = map(a, double);

// Print original array
puts(a);
// Print array with doubled values
puts(result);
// Fibonacci function using a space optimized approach.
let fibonacci = (number = 35) => {
    let mut a = 0;
    let mut b = 1;
    let mut c = 1;

    if (number == 0) {
        return a;
    }

	let mut i = 2;

	while(i <= number) {
        c = a + b
        a = b
        b = c
		i++;
	}

    return b;
};

puts(fibonacci())
// Fibonacci function using a space optimized approach.
let fibonacci = (number = 35) => {
    var a = 0;
    var b = 1;
    var c = 1;

    if (number == 0) {
        return a;
    }

	let mut i = 2;

	while(i <= number) {
        c = a + b
        a = b
        b = c
		i++;
	}

    return b;
};

puts(fibonacci());
